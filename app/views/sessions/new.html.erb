<h1>Login Page</h1>
<h2>Please enter your Username and Password below to login.</h2>

<p><%= flash[:alert] %>

<%= form_for(:user) do |f| %>
  
  <%= f.label :username %>
  <%= f.text_field :username %>
  <br>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  
  <%= f.submit "Login" %>
<% end %>